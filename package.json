{
    "name": "@aperturerobotics/timestamp",
    "private": true,
    "version": "0.2.0",
    "description": "timestamp object",
    "main": "timestamp.js",
    "types": "timestamp.d.ts",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
        "precommit": "lint-staged"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aperturerobotics/timestamp.git"
    },
    "author": "Christian Stewart <christian@aperturerobotics.com>",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/aperturerobotics/timestamp/issues"
    },
    "homepage": "https://github.com/aperturerobotics/timestamp#readme",
    "dependencies": {
        "protobufjs": "^6.8.0"
    },
    "lint-staged": {
        "**/*.ts": [
        "prettier --write --no-semi --single-quote",
        "git add"
        ]
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "../../node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(test|spec)\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "timestamp.js"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 95,
                "lines": 95,
                "statements": 95
            }
        },
        "collectCoverage": true,
        "mapCoverage": true
    },
    "devDependencies": {
        "@types/es6-promise": "^3.3.0",
        "@types/jest": "^21.1.0",
        "lint-staged": "^6.0.0",
        "prettier": "^1.4.4",
        "@types/node": "^8.0.0",
        "ts-jest": "^22.0.1"
    }
}
